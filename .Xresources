!!XTerm
XTerm.vt100.scrollBar: false
XTerm*internalBorder: 30


!! DPI
Xft.dpi: 109

!! URxvt Appearance
!!URxvt.font: xft:Iosevka:style=Regular:size=14
!!URxvt.boldFont: xft:Iosevka:style=Bold:size=14
!!URxvt.italicFont: xft:Iosevka:style=Italic:size=14
!!URxvt.boldItalicfont: xft:Iosevka:style=Bold Italic:size=14
*.font: xft:SF Mono:style=Regular:size=12,xft:Noto Serif CJK JP:style=SemiBold:size=12,xft:symbola:size=12:minspace=False
*.boldFont: xft:SF Mono:style=Bold:size=12,xft:Noto Serif CJK JP:style=Black:size=12
*.italicFont: xft:SF Mono:style=Regular Italic:size=12,xft:Noto Serif CJK JP:style=Italic:size=12
*.boldItalicFont: xft:SF Mono:style=Bold Italic:size=12,xft:Noto Serif CJK JP:style=Bold Italic:size=12
*antialias: true
*autohint: true
URxvt.letterSpace: -1
URxvt.lineSpace: 2
URxvt.geometry: 92x26
URxvt.internalBorder: 30
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.depth: 32
URxvt.background: [85]#2a2a2a
URxvt.urgentOnBell: true
URxvt.visualBell: false
URxvt.bell-command: notify-send -h string:x-canonical-private-synchronous:urxvtbell -i bell "urxvt" "bell"

!! Common Keybindings for Navigation
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D


URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1

!! Perls
URxvt.perl-ext-common : default,keyboard-select,resize-font,url-select,bell-command,confirm-paste,prevent-close

URxvt.resize-font.step: 0.5

URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: url-parse

!! Rofi
rofi.font: Noto Sans 11
